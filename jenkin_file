pipeline {
  agent { label 'slave1' } 
    stages {
        stage('Directory') {
            steps {
                sh 'git clone https://github.com/roopaabhi/node-hello'
            }
        }
        stage('Compile') {
            steps {
                sh 'npm compile'
            }
        }
        stage('package') {
            steps {
                sh 'npm start'
            }
        }
        
}
}
